.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")
.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.active{role: "presentation"}
          %a{href: "/prototypes/ranking"} Popular PROTO
        %li{role: "presentation"}
          %a{href: "/prototypes/latest"} Newest PROTO
      %button{type: "button", class: "btn btn-default col-lg-1", onclick: "location.href='/tags/'"} View Tags
.container.proto-list
  .row
    = render partial: "latest",collection: @prototypes

.text-center
  %ul.pagination
    = paginate @prototypes

